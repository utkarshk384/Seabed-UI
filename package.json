{
  "name": "seabed-ui",
  "version": "0.0.1",
  "description": "A simple component library made using Build time CSS-in-JS(linaria) library for best developer experience and user experience",
  "repository": "https://github.com/Utkarsh-Kumar384/Seabed-UI",
  "author": "Utkarsh Kumar <utkarshk384@gmail.com>",
  "license": "MIT",
  "private": true,
  "stream": true,
  "packages": [
    "packages/*"
  ],
  "workspaces": [
    "packages/*"
  ],
  "bootstrap": {
    "npmClientArgs": [
      "--no-package-lock"
    ]
  },
  "scripts": {
    "runs": "yarn manypkg run",
    "clean:modules": "rm -rf node_modules && lerna exec -- rm -rf node_modules",
    "clean:dist": "lerna exec -- yarn rimraf dist",
    "clean": "run-p clean:*",
    "bootstrap": "lerna bootstrap && yarn install",
    "start": "start-storybook -p 6006",
    "dev": "lerna exec yarn dev:start",
    "build": "lerna run build --stream",
    "build-storybook": "build-storybook",
    "test": "",
    "watch": "yarn lerna-watcher watch",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx packages --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "@linaria/babel-preset": "^3.0.0-beta.7",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/react": "^3.0.0-beta.7",
    "@linaria/shaker": "^3.0.0-beta.10",
    "@linaria/webpack-loader": "^3.0.0-beta.7"
  },
  "dependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@linaria/babel-preset": "^3.0.0-beta.7",
    "@linaria/core": "^3.0.0-beta.4",
    "@linaria/react": "^3.0.0-beta.7",
    "@linaria/shaker": "^3.0.0-beta.10",
    "@linaria/webpack-loader": "^3.0.0-beta.7",
    "@storybook/addon-a11y": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.3.6",
    "babel-loader": "^8.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "lodash": "^4.17.21",
    "walkjs": "^3.1.10"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.171",
    "@types/node": "^12.0.0",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.8",
    "@storybook/addon-console": "^1.2.3",
    "jest": "^27.0.6",
    "jest-axe": "^5.0.1",
    "ts-jest": "^27.0.5",
    "@types/jest-axe": "^3.5.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/testing-library__react": "^10.2.0",
    "prettier": "^2.2.1",
    "eslint": "^7.31.0",
    "eslint-plugin-jest-dom": "^3.9.0",
    "eslint-plugin-testing-library": "^4.11.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
